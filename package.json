{
	"name": "test-frontend",
	"description": "TEST TASK Frontend based on React",
	"license": "MIT",
	"private": true,
	"version": "1.0.15-SNAPSHOT",
	"scripts": {
		"build-dev": "tsc && vite build --mode dev",
		"build": "tsc && vite build",
		"commit": "cz",
		"start": "vite --open",
		"prod": "vite --open --mode prod",
		"preview": "vite preview",
		"test": "vitest",
		"test:ci": "vitest run",
		"format": "prettier -uw .",
		"run-tsc": "tsc",
		"run-eslint": "eslint --cache --fix --ext .ts,.tsx src",
		"run-stylelint": "stylelint --cache --fix '**/*.css'",
		"lint": "run-p run-tsc run-eslint run-stylelint"
	},
	"dependencies": {
		"@babel/core": "^7.17.8",
		"antd": "^5.11.1",
		"classnames": "^2.3.1",
		"debug": "^4.3.4",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-hook-form": "7.29.0",
		"react-input-mask": "^2.0.4",
		"react-router-dom": "6.2.1",
		"react-scripts": "^5.0.1",
		"react-select": "^5.2.2",
		"use-debounce": "^7.0.1"
	},
	"devDependencies": {
		"@nabla/vite-plugin-eslint": "1.4.0",
		"@tailwindcss/forms": "0.4.0",
		"@testing-library/dom": "8.11.3",
		"@testing-library/jest-dom": "5.16.2",
		"@testing-library/react": "12.1.3",
		"@testing-library/react-hooks": "7.0.2",
		"@testing-library/user-event": "13.5.0",
		"@types/css-mediaquery": "0.1.1",
		"@types/react": "17.0.39",
		"@types/react-dom": "17.0.11",
		"@types/react-input-mask": "^3.0.1",
		"@types/react-router-dom": "5.3.3",
		"@types/react-table": "^7.7.9",
		"@typescript-eslint/eslint-plugin": "5.12.0",
		"@typescript-eslint/parser": "5.12.0",
		"@vitejs/plugin-react": "1.2.0",
		"autoprefixer": "10.4.2",
		"c8": "7.11.0",
		"commitizen": "4.2.4",
		"css-mediaquery": "0.1.2",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "8.9.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "16.1.0",
		"eslint-config-prettier": "8.4.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-react": "7.28.0",
		"eslint-plugin-react-hooks": "4.3.0",
		"eslint-plugin-react-prefer-function-component": "1.0.0",
		"eslint-plugin-testing-library": "5.0.5",
		"eslint-plugin-unicorn": "41.0.0",
		"jsdom": "19.0.0",
		"lint-staged": "12.3.4",
		"msw": "0.38.1",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.6",
		"prettier": "2.5.1",
		"prettier-plugin-tailwindcss": "0.1.7",
		"react-error-overlay": "6.0.9",
		"rollup": "2.67.3",
		"rollup-plugin-istanbul": "3.0.0",
		"start-server-and-test": "1.14.0",
		"stylelint": "14.5.1",
		"stylelint-config-prettier": "9.0.3",
		"stylelint-config-standard": "25.0.0",
		"tailwindcss": "3.0.23",
		"typescript": "4.5.5",
		"vite": "2.8.4",
		"vite-plugin-pwa": "0.11.13",
		"vite-tsconfig-paths": "3.4.0",
		"vitest": "0.4.2",
		"whatwg-fetch": "3.6.2",
		"workbox-build": "6.4.2",
		"workbox-window": "6.4.2"
	},
	"browserslist": {
		"production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
		"development": [
			"last 1 chrome version",
			"last 1 firefox version"
		]
	},
	"lint-staged": {
		"*": "prettier -uw",
		"*.css": "stylelint --cache --fix",
		"*.{ts,tsx}": [
			"eslint --cache --fix",
			"vitest related --run --coverage=false"
		]
	},
	"resolutions": {
		"@types/react-paginate": "7.1.1",
		"@types/react": "17.0.39",
		"react-error-overlay": "6.0.9"
	}
}
